# platform :ios, '9.0'

target 'Seven' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Seven
  pod 'Firebase/Core'
  pod 'Firebase/AdMob'
  pod 'Google-Mobile-Ads-SDK'

  target 'SevenTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
    pod 'Firebase/AdMob'
    pod 'Google-Mobile-Ads-SDK'
  end

end
